            <td colspan="4" style="text-align: center">备注</td>
            <td colspan="6"><span style="font-family: 宋体; font-size: 12pt; mso-ansi-language: EN-US; mso-fareast-language: ZH-CN; mso-bidi-language: AR-SA; mso-bidi-font-family: 'Times New Roman'; mso-font-kerning: 1.0pt"><input classname="cn.myapps.runtime.dynaform.form.ejb.InputField" id="__AgMIcsZ6Xzpt04UOWts" text="text" style="width:650px" name="备注" texttype="text" fieldtype="VALUE_TYPE_VARCHAR" fieldkeyevent="Tabkey" readonlyshowvalonly="true" discript="" validaterule="" valuescript="var@nbsp;value1@nbsp;=@nbsp;getItemValueAsString(@quot;备注@quot;);@amp;#13;if(value1==null||value1.trim().length@lt;=0){@amp;#13;value1=@quot;配偶常驻地地址或所在单位地址：xx市xx区(县)xx路xx号@quot;;@amp;#13;}@amp;#13;value1;" hiddenscript="" hiddenvalue="" hiddenprintscript="" printhiddenvalue="" readonlyscript="" refreshonchanged="false" isdefaultvalue="false" calculateonrefresh="false" mobile="true" numberpattern="" processdescription="[];[]" filtercondition="" validatelibs="" editmode="01" refreshmode="0" refreshfields="" /></span></td>
