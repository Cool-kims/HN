  <filterScript><![CDATA[var userId = getWebUser().getId();
var sql = "select * from tlk_department_plan_copy2 where id in (select DOC_ID from T_ACTORRT where ACTORID='"+userId+"') and (STATELABEL = '部门副主任审核' or STATELABEL = '部门主任审批')";
var _pNo = getItemValue("_pNo");
var _pTitle = getItemValue("_pTitle");
