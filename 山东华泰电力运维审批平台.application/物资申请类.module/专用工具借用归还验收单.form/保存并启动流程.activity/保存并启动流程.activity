var rtn='';
var sql = "select * from tlk_专用工具借用归还验收单 where item_借用编号='"+com+"'  and istmp = 0";
var docs = queryBySQL(sql);
if(docs.iterator().hasNext()){
      var  rtn = "专用工具借用单已转换成归还验收单";
}
rtn;

]]></beforeActionScript>
