formname = filename.substring(filename.lastIndexOf("/")+1, filename.length);
}
if(number==null||number.trim().length<=0){
  number=getNextNo(formname,'编号','WZXJ');
  doc.findItem("编号").setValue(number);

}
]]></beforeActionScript>
